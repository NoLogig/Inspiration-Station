{
  "name": "nologig",
  "description": "Prototype MEAN Stack TypeScript Workspace",
  "version": "0.8.0",
  "engines": {
    "node": "12.6.0",
    "npm": "6.9.0"
  },
  "main": "./dist/electron/main.js",
  "scripts": {
    "ng": "ng",
    "build": "npm run build-client && npm run build-server",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "start": "ng serve -o",
    "test": "ng test",
    "tsc": "tsc",
    "electron": "electron .",
    "start-electron": "npm run build-electron && npm run electron",
    "start-http": "http-server -p 8080 -c-1 ./dist/client",
    "start-server": "concurrently \"tsc -w -p ./server\" \"nodemon ./dist/server/bin/www.js\" ",
    "build-client": "ng build --aot=true --prod --source-map=false",
    "build-electron": "ng build --aot=true --base-href=./ --prod --source-map=false && tsc -p ./electron",
    "build-server": "tsc -p ./server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.2",
    "@angular/cdk": "~8.1.3",
    "@angular/common": "~8.2.2",
    "@angular/compiler": "~8.2.2",
    "@angular/core": "~8.2.2",
    "@angular/forms": "~8.2.2",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "~8.2.2",
    "@angular/platform-browser-dynamic": "~8.2.2",
    "@angular/pwa": "~0.802.2",
    "@angular/router": "~8.2.2",
    "@angular/service-worker": "~8.2.2",
    "body-parser": "^1.19.0",
    "chart.js": "^2.8.0",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "googlemaps": "^1.12.0",
    "hammerjs": "^2.0.8",
    "mongodb": "^3.3.0",
    "morgan": "^1.9.1",
    "ng2-charts": "^2.3.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "0.9.1",
    "typescript": "3.6"
  },
  "devDependencies": {
    "lodash": "^4.17.15",
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "~8.2.2",
    "@angular/language-service": "~8.2.2",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/chart.js": "2.8.1",
    "@types/googlemaps": "^3.37.4",
    "@types/node": "~12.6.8",
    "@types/express": "^4.17.1",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/morgan": "^1.7.37",
    "@types/mongodb": "^3.3.0",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0"
  },
  "author": {
    "name": "Max Schweigerdt (NoLogig)",
    "url": "https://github.com/NoLogig"
  },
  "homepage": "https://inspiration-station.herokuapp.com/",
  "repository": {
